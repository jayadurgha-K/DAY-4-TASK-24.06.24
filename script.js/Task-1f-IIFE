// Task-1f
// Return median of two sorted arrays of the same size.

//! Using IIFE function


var arr6 = [2, 4, 6, 8, 0];
var arr7 = [1, 3, 5, 7, 9];

 (function (arr6, arr7) {
  let arr8 = [...arr6, ...arr7];
  //console.log(arr8);
  arr8.sort((a, b) => a - b);
  //console.log(arr8);
  let n = arr8.length;
  //console.log(n);
  if (n % 2 === 0) {
    console.log((arr8[n / 2] + arr8[n - 1] / 2) / 2);
  } else {
    console.log(arr8(Math.floor(n / 2)));
  }
})([2, 4, 6, 8, 0], [1, 3, 9, 7, 5]);