//Day-4-Task
// Take-1d
// Return all the prime numbers in an array


//! Using IIFE function

(function (array1) {
  var result7 = [];
  for (var i = 0; i < array1.length; i++) {
    var count = 0;
    for (var j = 2; j < array1[i]; j++) {
      if (array1[i] % j === 0) {
        count = 1;
      }
    }
    if (count === 0) {
      result7.push(array1[i]);
    }
  }
  console.log(result7);
})([2, 3, 4, 5, 6, 7, 8, 9, 10]);