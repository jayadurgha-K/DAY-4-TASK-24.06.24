
// Task-1g
//Remove duplicates from an array
//! using anonymous function

var result6 = [];

var duplicates = function (arr9) {
  for (var i = 0; i < arr9.length; i++) {
    if (result6.indexOf(arr9[i]) === -1) {
      result6.push(arr9[i]);
    }
  }
  return result6;
};

console.log(duplicates([1, 3, 5, 7, 9, 5, 9, 2, 6, 3, 4]));